getgenv().text = false

function text()
    spawn(function()
        while getgenv().text == true
            game:GetService("ReplicatedStorage").Events.SendTexts:FireServer("Phone")
            wait()
        end
    end)
end

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()
local w = library:CreateWindow("Texting Simulator")
local b = w:CreateFolder("Farming")
local c = w:CreateFolder("Eggs")
local d = w:CreateFolder("Teleports")

b:Toggle("Auto Text",function(bool)
    getgenv().text = bool
    print('Auto Text is: ', bool);
    if bool then
        text();
    end
end)
